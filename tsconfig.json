    "skipLibCheck": true,
    "composite": true,
    "outDir": "./dist",
    "rootDir": ".",
    "declaration": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "strict": false,
    "module": "commonjs",
    "target": "es2020",
